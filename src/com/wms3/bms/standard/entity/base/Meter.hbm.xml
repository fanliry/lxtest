<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>

    <class name="com.wms3.bms.standard.entity.base.Meter"  
           table="meter"
           dynamic-update="true"
           dynamic-insert="true">
        <id name="m_meterId" type="string" 
            unsaved-value="null">
            <column name="meterId" sql-type="char(38)" 
                    not-null="true"/>
            <generator class="uuid.hex"/>
        </id>        
        <property name="m_meterNumber">
            <column name="meterNumber" sql-type="string" 
                    not-null="false"/>
        </property>
        <property name="m_meterName">
            <column name="meterName" sql-type="string" 
                    not-null="false"/>
        </property>
        <property name="m_describe">
            <column name="describe" sql-type="string" 
                    not-null="false"/>
        </property>
     </class>	
</hibernate-mapping>
